{
  "kind": "collectionType",
  "collectionName": "properties",
  "info": {
    "singularName": "property",
    "pluralName": "properties",
    "displayName": "Property",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Villa",
        "Apartment"
      ],
      "required": true
    },
    "address": {
      "type": "text",
      "required": true
    },
    "state": {
      "type": "string",
      "required": true
    },
    "zipCode": {
      "type": "string",
      "required": true
    },
    "landmark": {
      "type": "string"
    },
    "latitude": {
      "type": "float"
    },
    "longitude": {
      "type": "float"
    },
    "startPrice": {
      "type": "decimal",
      "required": true
    },
    "size": {
      "type": "float",
      "required": true
    },
    "sizeUnit": {
      "type": "string",
      "default": "sqft"
    },
    "bedrooms": {
      "type": "integer",
      "required": true
    },
    "bathrooms": {
      "type": "integer",
      "required": true
    },
    "floors": {
      "type": "integer"
    },
    "reraId": {
      "type": "string"
    },
    "possessionDate": {
      "type": "date"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Under Construction",
        "Ready to Move",
        "New Launch"
      ]
    },
    "description": {
      "type": "richtext"
    },
    "photos": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "videos": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "videos"
      ]
    },
    "brochure": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "floorPlans": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "builder": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::builder.builder",
      "inversedBy": "properties"
    },
    "recommended_project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::recommended-project.recommended-project",
      "inversedBy": "properties"
    },
    "segregations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::segregation.segregation",
      "inversedBy": "properties"
    },
    "city": {
      "type": "enumeration",
      "enum": [
        "Bengaluru"
      ]
    },
    "endPrice": {
      "type": "decimal"
    },
    "bhks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::bhk.bhk"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
