{
  "kind": "collectionType",
  "collectionName": "properties",
  "info": {
    "singularName": "property",
    "pluralName": "properties",
    "displayName": "Property",
    "description": ""
  },
  "options": {
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Villa",
        "Apartment"
      ],
      "required": true
    },
    "address": {
      "type": "text",
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "state": {
      "type": "string",
      "required": true
    },
    "zipCode": {
      "type": "string",
      "required": true
    },
    "landmark": {
      "type": "string"
    },
    "latitude": {
      "type": "float"
    },
    "longitude": {
      "type": "float"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "currency": {
      "type": "string",
      "required": true
    },
    "size": {
      "type": "float",
      "required": true
    },
    "sizeUnit": {
      "type": "string",
      "default": "sqft"
    },
    "bedrooms": {
      "type": "integer",
      "required": true
    },
    "bathrooms": {
      "type": "integer",
      "required": true
    },
    "floors": {
      "type": "integer"
    },
    "developer": {
      "type": "string",
      "required": true
    },
    "reraId": {
      "type": "string"
    },
    "possessionDate": {
      "type": "date"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Under Construction",
        "Ready to Move"
      ]
    },
    "propertyNumber": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "photos": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images"
      ]
    },
    "videos": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "videos"
      ]
    },
    "brochure": {
      "type": "media",
      "allowedTypes": [
        "files"
      ]
    },
    "floorPlans": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images"
      ]
    },
    "builder": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::builder.builder",
      "inversedBy": "properties"
    },
    "recommended_project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::recommended-project.recommended-project",
      "inversedBy": "properties"
    }
  }
}
